class Human :

    def __init__(self, name, age):
        self.name = name
        self.age = age

    def check_adult(self):
        if self.age >= 20:
            print(f"{self.name}は大人です。")
        else:
            print(f"{self.name}は大人ではありません。")

human1 = Human("侍一郎", 25)
human1.check_adult()

human2 = Human("侍二郎", 22)
human2.check_adult()

human3 = Human("侍三郎", 19)
human3.check_adult()
