class Human :
    def __init__(printinfo):
        printinfo.name = ""
        printinfo.age = ""

    def set_name(printinfo, name):
        printinfo.name = name

    def show_name(printinfo):
        print(printinfo.name)

    def set_age(printinfo, age):
        printinfo.age = age

    def show_age(printinfo):
        print(printinfo.age)
